        <my:DatePicker Height="25" HorizontalAlignment="Left" Margin="133,23,0,0" Name="DTPIEGraphFrom" VerticalAlignment="Top" Width="115" xmlns:my="http://schemas.microsoft.com/wpf/2008/toolkit" Visibility="Collapsed" />
        <Label Height="28" HorizontalAlignment="Left" Margin="89,23,0,0" Name="label2" VerticalAlignment="Top" Width="49" Visibility="Collapsed">From :</Label>
        <Label Height="28" HorizontalAlignment="Left" Margin="259,23.139,0,0" Name="label3" VerticalAlignment="Top" Width="36" Visibility="Collapsed">To :</Label>
        <ComboBox Height="23" Margin="505,23,0,0" Name="CboProduct" VerticalAlignment="Top" HorizontalAlignment="Left" Width="120" Visibility="Collapsed" />
        <Label Height="28" HorizontalAlignment="Left" Margin="432,20,0,0" Name="label4" VerticalAlignment="Top" Width="62" Visibility="Collapsed">Product :</Label>
    </Grid>
</UserControl>
